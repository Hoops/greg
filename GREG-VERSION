# -*-shell-script-*-

PACKAGE=greg

GREG_MAJOR_VERSION=2
GREG_MINOR_VERSION=0
GREG_MICRO_VERSION=0

GREG_EFFECTIVE_VERSION=${GREG_MAJOR_VERSION}.${GREG_MINOR_VERSION}
GREG_VERSION=${GREG_EFFECTIVE_VERSION}.${GREG_MICRO_VERSION}
VERSION=${GREG_VERSION}

GREG_ITF_CURRENT=${GREG_MAJOR_VERSION}
GREG_ITF_REVISION=${GREG_MINOR_VERSION}
GREG_ITF_AGE=0
GREG_INTERFACE="${GREG_ITF_CURRENT}:${GREG_ITF_REVISION}:${GREG_ITF_AGE}"

